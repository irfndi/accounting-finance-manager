{
  "name": "finance-manager",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Corporate Finance Manager - A comprehensive financial management application",
  "main": "./alchemy.run.ts",
  "scripts": {
    "dev": "astro dev",
    "dev:worker": "alchemy run --env development",
    "dev:all": "pnpm dev",
    "build": "pnpm install --no-frozen-lockfile && astro check && astro build && echo '_worker.js' > dist/.assetsignore",
    "build:worker": "tsc --noEmit",
    "preview": "astro preview",
    "deploy": "pnpm dlx exec ./alchemy.run.ts",
    "deploy:wrangler": "wrangler deploy",
    "deploy:prod": "alchemy deploy --env production",
    "deploy:prod:wrangler": "wrangler deploy --env production",
    "destroy": "alchemy destroy",
    "test": "vitest run --workspace vitest.workspace.ts",
    "test:workers": "vitest run --workspace vitest.workspace.ts --project workers",
    "test:frontend": "vitest run --workspace vitest.workspace.ts --project frontend",
    "test:watch": "vitest --workspace vitest.workspace.ts",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:headless": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:all": "pnpm test && pnpm test:e2e",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "alchemy run --env development -- wrangler d1 migrations apply finance-manager-db",
    "db:migrate:prod": "alchemy run --env production -- wrangler d1 migrations apply finance-manager-db-prod",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:check": "drizzle-kit check",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "clean": "rm -rf dist .astro node_modules/.cache",
    "tail": "alchemy run --env development -- wrangler tail",
    "tail:prod": "alchemy run --env production -- wrangler tail",
    "types": "alchemy run -- wrangler types"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.55.0",
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.6.0",
    "@astrojs/node": "^9.2.2",
    "@astrojs/react": "^4.3.0",
    "@cloudflare/kv-asset-handler": "^0.4.0",
    "@hono/node-server": "^1.14.4",
    "@hono/zod-validator": "^0.7.0",
    "@noble/hashes": "^1.8.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "alchemy": "^0.41.2",
    "argon2": "^0.43.0",
    "astro": "^5.10.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "excelize-wasm": "^0.0.9",
    "exceljs": "^4.4.0",
    "hono": "^4.8.3",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.523.0",
    "openai": "^5.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.8.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@cloudflare/vite-plugin": "^1.8.0",
    "@cloudflare/vitest-pool-workers": "^0.8.48",
    "@cloudflare/workers-types": "^4.20250702.0",
    "@playwright/test": "^1.53.2",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "better-sqlite3": "^12.1.1",
    "concurrently": "^9.2.0",
    "drizzle-kit": "^0.31.4",
    "jsdom": "^26.1.0",
    "miniflare": "^4.20250617.4",
    "oxlint": "^1.5.0",
    "tailwindcss": "^4.1.11",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.22.0"
  },
  "overrides": {
    "undici@>=4.5.0 <5.28.5": ">=5.28.5",
    "dompurify@<3.2.4": ">=3.2.4",
    "esbuild@<=0.24.2": ">=0.25.0",
    "jspdf@<3.0.1": ">=3.0.1",
    "undici@<5.29.0": ">=5.29.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "keywords": [
    "finance",
    "accounting",
    "cloudflare",
    "astro",
    "react",
    "typescript",
    "d1",
    "worker"
  ]
}